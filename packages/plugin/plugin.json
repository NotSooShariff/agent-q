{
  "$schema": "https://schema.claude.com/plugin.json",
  "name": "agent-q",
  "version": "0.1.0",
  "displayName": "Agent Q",
  "description": "Enterprise-grade Agent Template Manager - Create and manage specialized AI agents with ease",
  "author": "Agent Q Team",
  "license": "MIT",
  "homepage": "https://github.com/yourusername/agent-q",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/agent-q.git"
  },
  "keywords": [
    "agents",
    "mcp",
    "templates",
    "ai",
    "claude-code"
  ],
  "mcpServers": {
    "agent-q": {
      "command": "npx",
      "args": ["-y", "@agent-q/mcp"]
    }
  },
  "agents": [
    {
      "name": "code-reviewer",
      "path": "agents/code-reviewer.md",
      "scope": "user"
    },
    {
      "name": "pair-programmer",
      "path": "agents/pair-programmer.md",
      "scope": "user"
    },
    {
      "name": "debugger",
      "path": "agents/debugger.md",
      "scope": "user"
    }
  ],
  "commands": [],
  "configuration": {
    "properties": {
      "agentQ.defaultScope": {
        "type": "string",
        "enum": ["project", "user"],
        "default": "project",
        "description": "Default scope for new agents"
      },
      "agentQ.templateDirectory": {
        "type": "string",
        "description": "Custom template directory path"
      }
    }
  }
}
